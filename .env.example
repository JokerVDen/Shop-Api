COMPOSE_PROJECT_NAME=shop_app

APP_NAME="RESTful API"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://api.shop.loc
DOCKER_NGINX_DOMAIN_NAME=api.shop.loc

LOG_CHANNEL=stack

FILESYSTEM_DRIVER=images

HTTP_PORT=80
HTTP_PORT_DOCKER=8080

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_PORT_DOCKER=33061
DB_DATABASE=rest_api
DB_USERNAME=user
DB_PASSWORD=123456

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_PORT_DOCKER=63791

MAIL_MAILER=log
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

LINUX_UID=1000
LINUX_GID=1000

PMA_DOCKER_PORT=8081
PMA_PORT=80

SWAGGER_PORT_DOCKER=8082
SWAGGER_PORT=8080
SWAGGER_FILE=/usr/share/nginx/mnt/swagger.yaml
SWAGGER_VOLUME_DOCKER_DIRECTORY=/usr/share/nginx/mnt
SWAGGER_VOLUME_LOCAL_DIRECTORY=./swagger
